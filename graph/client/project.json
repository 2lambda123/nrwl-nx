{
  "name": "graph-client",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "graph/client/src",
  "projectType": "application",
  "targets": {
    "generate-dev-environment-js": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "ts-node -P ./scripts/tsconfig.scripts.json ./scripts/generate-graph-environment.ts"
        ]
      }
    },
    "generate-graph-base": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "ts-node -P ./scripts/tsconfig.scripts.json ./scripts/generate-graph.ts"
        ]
      }
    },
    "generate-graph": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npx nx run graph-client:generate-graph-base",
          "npx nx run graph-client:generate-dev-environment-js"
        ],
        "parallel": false
      }
    },
    "build-base": {
      "executor": "@nrwl/vite:build",
      "options": {
        "outputPath": "build/apps/graph"
      },
      "configurations": {
        "dev": {
          "optimization": false,
          "sourceMap": true,
          "extractCss": false,
          "namedChunks": true,
          "extractLicenses": false,
          "vendorChunk": true
        },
        "release": {
          "optimization": true,
          "sourceMap": false,
          "extractCss": true,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "2mb",
              "maximumError": "5mb"
            }
          ]
        }
      },
      "defaultConfiguration": "release",
      "outputs": ["{options.outputPath}"]
    },
    "serve-base": {
      "executor": "@nrwl/vite:dev-server",
      "configurations": {
        "dev": {
          "buildTarget": "graph-client:build-base:dev",
          "port": 4201,
          "mode": "development"
        },
        "nx-console": {
          "buildTarget": "graph-client:build-base:dev",
          "port": 4202,
          "mode": "nx-console"
        },
        "release": {
          "buildTarget": "graph-client:build-base:release",
          "port": 4203,
          "mode": "release"
        },
        "watch": {
          "buildTarget": "graph-client:build-base:dev",
          "port": 4204,
          "mode": "watch"
        },
        "release-static": {
          "buildTarget": "graph-client:build-base:release",
          "port": 4205,
          "mode": "release-static"
        },
        "dev-e2e": {
          "buildTarget": "graph-client:build-base:dev",
          "port": 4206,
          "mode": "dev-e2e"
        }
      },
      "defaultConfiguration": "dev",
      "options": {
        "buildTarget": "graph-client:build"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "configurations": {
        "dev": {
          "commands": [
            "npx nx run graph-client:generate-dev-environment-js",
            "npx nx serve-base graph-client"
          ],
          "parallel": false
        },
        "release": {
          "commands": ["npx nx serve-base graph-client --configuration release"]
        },
        "release-static": {
          "commands": [
            "npx nx serve-base graph-client --configuration release-static"
          ]
        },
        "watch": {
          "commands": ["npx nx serve-base graph-client --configuration watch"]
        },
        "nx-console": {
          "commands": [
            "npx nx serve-base graph-client --configuration nx-console"
          ]
        }
      },
      "defaultConfiguration": "dev"
    },
    "lint": {},
    "test": {
      "executor": "@nrwl/vite:test"
    },
    "storybook": {
      "executor": "@nrwl/storybook:storybook",
      "options": {
        "uiFramework": "@storybook/react",
        "port": 4400,
        "configDir": "graph/client/.storybook"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@nrwl/storybook:build",
      "outputs": ["{options.outputDir}"],
      "options": {
        "uiFramework": "@storybook/react",
        "configDir": "graph/client/.storybook",
        "outputDir": "dist/storybook/graph-client"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "preview": {
      "executor": "@nrwl/vite:preview-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "graph-client:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "graph-client:build:development"
        },
        "production": {
          "buildTarget": "graph-client:build:production"
        }
      }
    }
  },
  "tags": []
}
