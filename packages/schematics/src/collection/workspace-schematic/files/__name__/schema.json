{
  "$schema": "http://json-schema.org/schema",
  "id": "<%= name %>",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Library name"
    },
    "directory": {
      "type": "string",
      "description": "The container directory for the '<%= name %>' library that will be created."
    }
  },
  "required": [
    "name"
  ]
}
