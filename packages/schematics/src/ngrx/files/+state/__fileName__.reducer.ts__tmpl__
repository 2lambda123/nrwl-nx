import {<%= className %>} from './<%= fileName %>.interfaces';
import * as <%= propertyName %> from './<%= fileName %>.actions';

export function <%= propertyName %>Reducer(state: <%= className %>, action: <%= propertyName %>.Action): <%= className %> {
  switch (action.type) {
    case <%= propertyName %>.LOAD_SUCCESS: {
      return {...state, ...action.payload};
    }
    default: {
      return state;
    }
  }
}
