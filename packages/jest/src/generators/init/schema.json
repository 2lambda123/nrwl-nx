{
  "$schema": "http://json-schema.org/schema",
  "$id": "NxJestInit",
  "cli": "nx",
  "title": "Jest init",
  "description": "Add Jest Configuration to a workspace.",
  "type": "object",
  "properties": {
    "skipFormat": {
      "description": "Skip formatting files.",
      "type": "boolean",
      "default": false,
      "x-priority": "internal"
    },
    "skipPackageJson": {
      "type": "boolean",
      "default": false,
      "description": "Do not add dependencies to `package.json`.",
      "x-priority": "internal"
    }
  },
  "required": []
}
