    "plugin": {
      "factory": "./src/schematics/plugin/plugin#pluginGenerator",
      "schema": "./src/schematics/plugin/schema.json",
      "description": "Create a Nx Plugin"
    },
    "e2e-project": {
      "factory": "./src/schematics/e2e-project/e2e#e2eGenerator",
